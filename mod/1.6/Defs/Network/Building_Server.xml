    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Things/Building/Ship/ShipComputerCore</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
      <shadowData>
        <volume>(2.76, 0.5, 2.66)</volume>
